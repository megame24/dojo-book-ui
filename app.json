{
  "expo": {
    "name": "Dojologs",
    "slug": "dojologs",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./src/assets/dojologs-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/dojologs-splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#2980B9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.megame.dojologs",
      "googleServicesFile": "./GoogleService-info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/dojologs-adaptive-icon.png",
        "backgroundColor": "#2980B9"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.megame.dojologs"
    },
    "web": {
      "favicon": "./src/assets/dojologs-favicon.png"
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      "expo-apple-authentication"
    ],
    "extra": {
      "eas": {
        "projectId": "a793950b-3899-4ad9-8549-c6e46ca582a3"
      }
    },
    "owner": "megame"
  }
}
